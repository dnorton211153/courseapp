<div class="modal fade" id="modalModules" tabindex="-1" role="dialog" aria-labelledby="modalModules" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalModulesLabel">Module Order</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        {{#each quizzes}}
        <div class="row">
          <div id="{{_id}}" class="col-10">{{name}}</div>
          <div class="col-1 orderLower"><i class="fa fa-caret-down" aria-hidden="true"></i></div>
          <div class="col-1 orderHigher"><i class="fa fa-caret-up" aria-hidden="true"></i></div>
        </div>
        {{/each}}
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Done</button>
        {{!-- <button type="button" class="btn btn-primary">Done</button> --}}
      </div>
    </div>
  </div>
</div>