<div id="myActivityOuterAccordian" role="tablist" aria-multiselectable="true">
    <div class="card">
        <div class="card-header" role="tab" id="myActivityHeader">
            <h5 class="mb-0">
                <a data-toggle="collapse" data-parent="#myActivityOuterAccordian" href="#myActivityContent" aria-expanded="true" aria-controls="myActivityContent">
          My Activities
        </a>
            </h5>
        </div>
        <div id="myActivityContent" class="collapse in" role="tabpanel" aria-labelledby="myActivityHeader">

            <div id="myActivityInnerAccordian" role="tablist" aria-multiselectable="true" class="ml-5">
                {{#each scoreArraysObj}}
                <div class="card">
                    <div class="card-header" role="tab" id="">
                        <h5 class="mb-0">
                            <a data-toggle="collapse" data-parent="#myActivityInnerAccordian"
                                href="#section{{@index}}Content" aria-expanded="true"
                                aria-controls="section{{@index}}Content">
                                <div class="row">
                                    <div class="col-5">{{@key}}</div>    
                                    <div class="col-2">{{averageScore}}</div>
                                    <div class="col-2">{{{secondsToMMSS overallTime}}}</div>
                                    <div class="col-3">{{{shortDate lastQuiz}}}</div>
                                </div>
                                    
                            </a>
                        </h5>
                    </div>

                    <div id="section{{@index}}Content" class="collapse in" role="tabpanel"
                        aria-labelledby="section{{@index}}Header">
                        {{#each quizzes}}
                        <div class="card-body py-2">
                                <form class="row userQuiz">
                                    
                                    <input type="hidden" name="userQuizId" value="{{id}}">
                                    <input type="hidden" name="quizId" value="{{quizId}}">
                                    <div class="col-5 noclick">{{name}}</div>    
                                    <div class="col-2 noclick">{{score}}</div>
                                    <div class="col-2 noclick">{{{secondsToMMSS timePassed}}}</div>
                                    <div class="col-3 noclick">{{{shortDate date}}}</div>
                                    
                                </form>
                        </div>
                        {{/each}}
                    </div>

                </div>
                {{/each}}

            </div>

        </div>
    </div>
</div>

